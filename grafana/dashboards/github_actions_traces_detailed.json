{
  "id": null,
  "title": "GitHub Actions - Trace Details",
  "tags": [
    "github",
    "actions",
    "traces",
    "otel",
    "detailed"
  ],
  "timezone": "browser",
  "description": "Detailed trace-level analysis of GitHub Actions workflow execution with step-by-step insights",
  "panels": [
    {
      "id": 1,
      "title": "Recent Workflow Executions",
      "type": "table",
      "targets": [
        {
          "expr": "sum by (span_name, service_name, span_name, status_code) (github_actions_traces_span_metrics_calls_total)",
          "refId": "A",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "left"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "span_name"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Pipeline"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "service_name"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Repository"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "span_name"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Step"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "status_code"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Status"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Value"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Executions"
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Step Duration Analysis",
      "type": "graph",
      "targets": [
        {
          "expr": "sum by (span_name) (rate(github_actions_traces_span_metrics_duration_seconds_sum[5m])) / sum by (span_name) (rate(github_actions_traces_span_metrics_duration_seconds_count[5m]))",
          "refId": "A",
          "legendFormat": "{{span_name}}"
        }
      ],
      "yAxes": [
        {
          "unit": "s",
          "label": "Average Duration"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 10
      }
    },
    {
      "id": 3,
      "title": "Success Rate by Step",
      "type": "table",
      "targets": [
        {
          "expr": "sum by (span_name) (github_actions_traces_span_metrics_calls_total{status_code=\"STATUS_CODE_OK\"}) / sum by (span_name) (github_actions_traces_span_metrics_calls_total) * 100",
          "refId": "A",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "custom": {
            "align": "left"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "span_name"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Step Name"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Value"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Success Rate %"
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 18
      }
    },
    {
      "id": 4,
      "title": "Pipeline Activity Heatmap",
      "type": "heatmap",
      "targets": [
        {
          "expr": "sum by (span_name) (rate(github_actions_traces_span_metrics_calls_total[5m]))",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 18
      }
    },
    {
      "id": 5,
      "title": "Execution Timeline",
      "type": "graph",
      "targets": [
        {
          "expr": "sum by (span_name) (rate(github_actions_traces_span_metrics_calls_total[1m]))",
          "refId": "A",
          "legendFormat": "{{span_name}}"
        }
      ],
      "yAxes": [
        {
          "unit": "short",
          "label": "Executions per minute"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 26
      }
    },
    {
      "id": 6,
      "title": "Step Duration Distribution",
      "type": "graph",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (span_name, le) (rate(github_actions_traces_span_metrics_duration_seconds_bucket[5m])))",
          "refId": "A",
          "legendFormat": "{{span_name}} - 50th"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (span_name, le) (rate(github_actions_traces_span_metrics_duration_seconds_bucket[5m])))",
          "refId": "B",
          "legendFormat": "{{span_name}} - 95th"
        }
      ],
      "yAxes": [
        {
          "unit": "s",
          "label": "Duration"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 34
      }
    },
    {
      "id": 7,
      "title": "Repository Activity Breakdown",
      "type": "piechart",
      "targets": [
        {
          "expr": "sum by (service_name) (github_actions_traces_span_metrics_calls_total)",
          "refId": "A",
          "legendFormat": "{{service_name}}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 42
      }
    },
    {
      "id": 8,
      "title": "Error Frequency by Step",
      "type": "table",
      "targets": [
        {
          "expr": "sum by (span_name, status_code) (github_actions_traces_span_metrics_calls_total{status_code!=\"STATUS_CODE_OK\"})",
          "refId": "A",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "left"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "span_name"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Step Name"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "status_code"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Error Type"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Value"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Error Count"
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 42
      }
    }
  ],
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "refresh": "10s"
}