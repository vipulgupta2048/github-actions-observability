{
  "id": null,
  "uid": "gh-repo-performance",
  "title": "GitHub Actions - Repository Performance",
  "tags": [
    "github",
    "actions",
    "repository",
    "performance",
    "sla",
    "cost"
  ],
  "timezone": "browser",
  "editable": true,
  "graphTooltip": 1,
  "description": "Repository performance dashboard with resource utilization, cost analysis, dependency tracking, and SLA compliance monitoring",
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "30s",
      "1m",
      "5m",
      "15m",
      "30m",
      "1h",
      "2h",
      "1d"
    ],
    "quick_ranges": [
      {
        "display": "Last 7 days",
        "from": "now-24h",
        "to": "now"
      },
      {
        "display": "Last 30 days",
        "from": "now-24h",
        "to": "now"
      },
      {
        "display": "Last 90 days",
        "from": "now-24h",
        "to": "now"
      }
    ]
  },
  "templating": {
    "list": [
      {
        "allValue": ".*",
        "current": {
          "text": "All",
          "value": "$__all"
        },
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "definition": "label_values(github_actions_traces_span_metrics_calls_total, service_name)",
        "includeAll": true,
        "multi": true,
        "name": "service",
        "options": [],
        "query": "label_values(github_actions_traces_span_metrics_calls_total, service_name)",
        "refresh": 2,
        "regex": "",
        "sort": 1,
        "type": "query",
        "label": "Service"
      },
      {
        "current": {
          "text": "95",
          "value": "95"
        },
        "options": [
          {
            "text": "90",
            "value": "90"
          },
          {
            "text": "95",
            "value": "95"
          },
          {
            "text": "99",
            "value": "99"
          }
        ],
        "name": "sla_threshold",
        "type": "custom",
        "label": "SLA Threshold %"
      },
      {
        "current": {
          "text": "0.02",
          "value": "0.02"
        },
        "options": [
          {
            "text": "0.01",
            "value": "0.01"
          },
          {
            "text": "0.02",
            "value": "0.02"
          },
          {
            "text": "0.05",
            "value": "0.05"
          }
        ],
        "name": "cost_per_minute",
        "type": "custom",
        "label": "Cost per CPU minute ($)"
      },
      {
        "current": {
          "text": "7d",
          "value": "7d"
        },
        "options": [
          {
            "text": "1d",
            "value": "1d"
          },
          {
            "text": "7d",
            "value": "7d"
          },
          {
            "text": "30d",
            "value": "30d"
          }
        ],
        "name": "analysis_window",
        "type": "custom",
        "label": "Analysis Window"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "Repository Resource Utilization (CPU Minutes Estimated)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum by (service_name) (rate(github_actions_traces_span_metrics_duration_seconds_sum{service_name=~\"$service\"}[1h])) * 60 / 60",
          "refId": "A",
          "legendFormat": "{{service_name}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "min",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20,
            "stacking": {
              "mode": "normal"
            }
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 1000
              },
              {
                "color": "red",
                "value": 5000
              }
            ]
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": [
            "last",
            "max",
            "mean"
          ]
        }
      },
      "gridPos": {
        "h": 9,
        "w": 24,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Cost Analysis per Repository",
      "type": "table",
      "targets": [
        {
          "expr": "sum by (service_name) (increase(github_actions_traces_span_metrics_duration_seconds_sum{service_name=~\"$service\"}[$analysis_window])) / 60 * $cost_per_minute",
          "refId": "A",
          "format": "table",
          "instant": true,
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "sum by (service_name) (github_actions_traces_span_metrics_calls_total{step_status=\"success\",service_name=~\"$service\"})",
          "refId": "B",
          "format": "table",
          "instant": true,
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "(sum by (service_name) (increase(github_actions_traces_span_metrics_duration_seconds_sum{service_name=~\"$service\"}[$analysis_window])) / 60 * $cost_per_minute) / sum by (service_name) (github_actions_traces_span_metrics_calls_total{step_status=\"success\",service_name=~\"$service\"})",
          "refId": "C",
          "format": "table",
          "instant": true,
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "left"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "service_name"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Repository"
              },
              {
                "id": "custom.width",
                "value": 150
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "Value #A"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Total Cost ($)"
              },
              {
                "id": "unit",
                "value": "currencyUSD"
              },
              {
                "id": "custom.displayMode",
                "value": "color-background"
              },
              {
                "id": "thresholds",
                "value": {
                  "steps": [
                    {
                      "color": "green",
                      "value": 0
                    },
                    {
                      "color": "yellow",
                      "value": 100
                    },
                    {
                      "color": "red",
                      "value": 500
                    }
                  ]
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "Value #B"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Successful Deployments"
              },
              {
                "id": "unit",
                "value": "short"
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "Value #C"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Cost per Success ($)"
              },
              {
                "id": "unit",
                "value": "currencyUSD"
              },
              {
                "id": "custom.displayMode",
                "value": "gradient-gauge"
              }
            ]
          }
        ]
      },
      "transformations": [
        {
          "id": "filterFieldsByName",
          "options": {
            "include": {
              "names": [
                "service_name",
                "Value #A",
                "Value #B",
                "Value #C"
              ]
            }
          }
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 9
      }
    },
    {
      "id": 3,
      "title": "Workflow Frequency Heatmap",
      "type": "heatmap",
      "targets": [
        {
          "expr": "sum by (service_name, workflow_step) (increase(github_actions_traces_span_metrics_calls_total{service_name=~\"$service\"}[1h]))",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            }
          }
        }
      },
      "options": {
        "calculate": false,
        "cellGap": 1,
        "color": {
          "exponent": 0.5,
          "fill": "dark-blue",
          "mode": "spectrum",
          "reverse": false,
          "scale": "exponential",
          "scheme": "Blues",
          "steps": 64
        },
        "exemplars": {
          "color": "rgba(255,0,255,0.7)"
        },
        "filterValues": {
          "le": 1e-09
        },
        "legend": {
          "show": true
        },
        "rowsFrame": {
          "layout": "auto"
        },
        "tooltip": {
          "show": true,
          "yHistogram": false
        },
        "yAxis": {
          "axisPlacement": "left",
          "reverse": false
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 9
      }
    },
    {
      "id": 4,
      "title": "SLA Compliance Tracking",
      "type": "stat",
      "targets": [
        {
          "expr": "100 * (sum by (service_name) (github_actions_traces_span_metrics_calls_total{step_status=\"success\",service_name=~\"$service\"}) / sum by (service_name) (github_actions_traces_span_metrics_calls_total{service_name=~\"$service\"}))",
          "refId": "A",
          "legendFormat": "{{service_name}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "yellow",
                "value": "$sla_threshold"
              },
              {
                "color": "green",
                "value": 99
              }
            ]
          },
          "custom": {
            "orientation": "horizontal",
            "textMode": "auto"
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ],
          "fields": ""
        },
        "text": {
          "titleSize": 14,
          "valueSize": 24
        },
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center"
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 17
      }
    },
    {
      "id": 5,
      "title": "Repository Dependencies Impact Analysis",
      "type": "nodeGraph",
      "targets": [
        {
          "expr": "sum by (service_name) (github_actions_traces_span_metrics_calls_total{service_name=~\"$service\"})",
          "refId": "A",
          "format": "table",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            }
          }
        }
      },
      "options": {
        "nodes": {
          "mainStatUnit": "short",
          "secondaryStatUnit": "short",
          "arcs": [
            {
              "field": "Value",
              "color": "green"
            }
          ]
        },
        "edges": {
          "mainStatUnit": "short",
          "secondaryStatUnit": "short"
        }
      },
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 25
      }
    },
    {
      "id": 6,
      "title": "Performance Trends by Repository",
      "type": "timeseries",
      "targets": [
        {
          "expr": "avg by (service_name) (rate(github_actions_traces_span_metrics_duration_seconds_sum{service_name=~\"$service\"}[1h]) / rate(github_actions_traces_span_metrics_duration_seconds_count{service_name=~\"$service\"}[1h]))",
          "refId": "A",
          "legendFormat": "Avg Duration - {{service_name}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "100 * (sum by (service_name) (rate(github_actions_traces_span_metrics_calls_total{step_status=\"success\",service_name=~\"$service\"}[1h])) / sum by (service_name) (rate(github_actions_traces_span_metrics_calls_total{service_name=~\"$service\"}[1h])))",
          "refId": "B",
          "legendFormat": "Success Rate - {{service_name}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byRegexp",
              "options": "Avg Duration.*"
            },
            "properties": [
              {
                "id": "unit",
                "value": "s"
              },
              {
                "id": "custom.axisPlacement",
                "value": "left"
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "Success Rate.*"
            },
            "properties": [
              {
                "id": "unit",
                "value": "percent"
              },
              {
                "id": "custom.axisPlacement",
                "value": "right"
              },
              {
                "id": "min",
                "value": 0
              },
              {
                "id": "max",
                "value": 100
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 25
      }
    },
    {
      "id": 7,
      "title": "Resource Efficiency Metrics",
      "type": "table",
      "targets": [
        {
          "expr": "sum by (service_name) (increase(github_actions_traces_span_metrics_calls_total{service_name=~\"$service\"}[$analysis_window]))",
          "refId": "A",
          "format": "table",
          "instant": true,
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "sum by (service_name) (increase(github_actions_traces_span_metrics_duration_seconds_sum{service_name=~\"$service\"}[$analysis_window])) / 60",
          "refId": "B",
          "format": "table",
          "instant": true,
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "(sum by (service_name) (increase(github_actions_traces_span_metrics_duration_seconds_sum{service_name=~\"$service\"}[$analysis_window])) / 60) / sum by (service_name) (increase(github_actions_traces_span_metrics_calls_total{service_name=~\"$service\"}[$analysis_window]))",
          "refId": "C",
          "format": "table",
          "instant": true,
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "100 * (sum by (service_name) (github_actions_traces_span_metrics_calls_total{step_status=\"success\",service_name=~\"$service\"}) / sum by (service_name) (github_actions_traces_span_metrics_calls_total{service_name=~\"$service\"}))",
          "refId": "D",
          "format": "table",
          "instant": true,
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "left"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "service_name"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Repository"
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "Value #A"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Total Runs"
              },
              {
                "id": "unit",
                "value": "short"
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "Value #B"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "CPU Minutes"
              },
              {
                "id": "unit",
                "value": "min"
              },
              {
                "id": "custom.displayMode",
                "value": "gradient-gauge"
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "Value #C"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Avg Minutes/Run"
              },
              {
                "id": "unit",
                "value": "min"
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "Value #D"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Success Rate %"
              },
              {
                "id": "unit",
                "value": "percent"
              },
              {
                "id": "custom.displayMode",
                "value": "color-background"
              },
              {
                "id": "thresholds",
                "value": {
                  "steps": [
                    {
                      "color": "red",
                      "value": 0
                    },
                    {
                      "color": "yellow",
                      "value": 90
                    },
                    {
                      "color": "green",
                      "value": 95
                    }
                  ]
                }
              }
            ]
          }
        ]
      },
      "transformations": [
        {
          "id": "filterFieldsByName",
          "options": {
            "include": {
              "names": [
                "service_name",
                "Value #A",
                "Value #B",
                "Value #C",
                "Value #D"
              ]
            }
          }
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 34
      }
    },
    {
      "id": 8,
      "title": "SLA Breach Alerts",
      "type": "table",
      "targets": [
        {
          "expr": "(100 * (sum by (service_name) (github_actions_traces_span_metrics_calls_total{step_status=\"success\",service_name=~\"$service\"}) / sum by (service_name) (github_actions_traces_span_metrics_calls_total{service_name=~\"$service\"}))) < $sla_threshold",
          "refId": "A",
          "format": "table",
          "instant": true,
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "left"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "service_name"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Repository"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Value"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "SLA Breach"
              },
              {
                "id": "custom.displayMode",
                "value": "color-background"
              },
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "red"
                }
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 6,
        "w": 24,
        "x": 0,
        "y": 42
      }
    }
