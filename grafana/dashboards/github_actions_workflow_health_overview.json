{
  "id": null,
  "uid": "gh-workflow-health",
  "title": "GitHub Actions - Workflow Health Overview",
  "tags": [
    "github",
    "actions",
    "health",
    "overview",
    "ci/cd"
  ],
  "timezone": "browser",
  "editable": true,
  "graphTooltip": 1,
  "description": "Primary dashboard for GitHub Actions workflow health monitoring with success rates, duration analysis, and repository comparison",
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m",
      "15m",
      "30m",
      "1h",
      "2h",
      "1d"
    ],
    "quick_ranges": [
      {
        "display": "Last 24 hours",
        "from": "now-24h",
        "to": "now"
      },
      {
        "display": "Last 7 days",
        "from": "now-24h",
        "to": "now"
      },
      {
        "display": "Last 30 days",
        "from": "now-24h",
        "to": "now"
      }
    ]
  },
  "templating": {
    "list": [
      {
        "allValue": ".*",
        "current": {
          "text": "All",
          "value": "$__all"
        },
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "definition": "label_values(github_actions_traces_span_metrics_calls_total, service_name)",
        "includeAll": true,
        "multi": true,
        "name": "service",
        "options": [],
        "query": "label_values(github_actions_traces_span_metrics_calls_total, service_name)",
        "refresh": 2,
        "regex": "",
        "sort": 1,
        "type": "query"
      },
      {
        "allValue": ".*",
        "current": {
          "text": "All",
          "value": "$__all"
        },
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "definition": "label_values(github_actions_traces_span_metrics_calls_total{service_name=~\"$service\"}, workflow_step)",
        "includeAll": true,
        "multi": true,
        "name": "workflow_step",
        "options": [],
        "query": "label_values(github_actions_traces_span_metrics_calls_total{service_name=~\"$service\"}, workflow_step)",
        "refresh": 2,
        "regex": "",
        "sort": 1,
        "type": "query"
      },
      {
        "current": {
          "text": "7d",
          "value": "7d"
        },
        "options": [
          {
            "text": "1d",
            "value": "1d"
          },
          {
            "text": "7d",
            "value": "7d"
          },
          {
            "text": "30d",
            "value": "30d"
          }
        ],
        "name": "time_range",
        "type": "custom"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "Workflow Success Rate - 7 Day Trend",
      "type": "timeseries",
      "targets": [
        {
          "expr": "100 * (sum(rate(github_actions_traces_span_metrics_calls_total{step_status=\"success\",service_name=~\"$service\",workflow_step=~\"$workflow_step\"}[1h])) by (workflow_step) / sum(rate(github_actions_traces_span_metrics_calls_total{service_name=~\"$service\",workflow_step=~\"$workflow_step\"}[1h])) by (workflow_step))",
          "refId": "A",
          "legendFormat": "{{workflow_step}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 80
              },
              {
                "color": "green",
                "value": 95
              }
            ]
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "spanNulls": false
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "values": [
            "last",
            "mean"
          ]
        }
      },
      "gridPos": {
        "h": 9,
        "w": 24,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Top 10 Failing Workflows",
      "type": "table",
      "targets": [
        {
          "expr": "topk(10, sum by (workflow_step, service_name) (github_actions_traces_span_metrics_calls_total{step_status!=\"success\",service_name=~\"$service\",workflow_step=~\"$workflow_step\"}))",
          "refId": "A",
          "format": "table",
          "instant": true,
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "100 * (1 - (sum by (workflow_step, service_name) (github_actions_traces_span_metrics_calls_total{step_status=\"success\",service_name=~\"$service\",workflow_step=~\"$workflow_step\"}) / sum by (workflow_step, service_name) (github_actions_traces_span_metrics_calls_total{service_name=~\"$service\",workflow_step=~\"$workflow_step\"})))",
          "refId": "B",
          "format": "table",
          "instant": true,
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "left"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "workflow_step"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Workflow Step"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "service_name"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Repository"
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "Value #A"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Failure Count"
              },
              {
                "id": "custom.displayMode",
                "value": "color-background"
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "Value #B"
            },
            "properties": [
              {
                "id": "displayName",
                "value": "Failure Rate %"
              },
              {
                "id": "unit",
                "value": "percent"
              },
              {
                "id": "custom.displayMode",
                "value": "color-background"
              },
              {
                "id": "thresholds",
                "value": {
                  "steps": [
                    {
                      "color": "green",
                      "value": 0
                    },
                    {
                      "color": "yellow",
                      "value": 5
                    },
                    {
                      "color": "red",
                      "value": 20
                    }
                  ]
                }
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 9
      }
    },
    {
      "id": 3,
      "title": "Workflow Duration Percentiles",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(github_actions_traces_span_metrics_duration_seconds_bucket{service_name=~\"$service\",workflow_step=~\"$workflow_step\"}[5m])) by (le, workflow_step))",
          "refId": "A",
          "legendFormat": "P50 - {{workflow_step}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(github_actions_traces_span_metrics_duration_seconds_bucket{service_name=~\"$service\",workflow_step=~\"$workflow_step\"}[5m])) by (le, workflow_step))",
          "refId": "B",
          "legendFormat": "P95 - {{workflow_step}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(github_actions_traces_span_metrics_duration_seconds_bucket{service_name=~\"$service\",workflow_step=~\"$workflow_step\"}[5m])) by (le, workflow_step))",
          "refId": "C",
          "legendFormat": "P99 - {{workflow_step}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        }
      },
      "alert": {
        "conditions": [
          {
            "evaluator": {
              "params": [
                600
              ],
              "type": "gt"
            },
            "operator": {
              "type": "and"
            },
            "query": {
              "params": [
                "C",
                "5m",
                "now"
              ]
            },
            "reducer": {
              "params": [],
              "type": "avg"
            },
            "type": "query"
          }
        ],
        "executionErrorState": "alerting",
        "for": "5m",
        "frequency": "10s",
        "handler": 1,
        "name": "Workflow Duration P99 Alert",
        "noDataState": "no_data",
        "notifications": []
      },
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 9
      }
    },
    {
      "id": 4,
      "title": "Queue vs Execution Time Analysis",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(github_actions_traces_span_metrics_duration_seconds_sum{workflow_step=~\".*queue.*|.*waiting.*\",service_name=~\"$service\",workflow_step=~\"$workflow_step\"}[5m])) by (workflow_step) / sum(rate(github_actions_traces_span_metrics_duration_seconds_count{workflow_step=~\".*queue.*|.*waiting.*\",service_name=~\"$service\",workflow_step=~\"$workflow_step\"}[5m])) by (workflow_step)",
          "refId": "A",
          "legendFormat": "Queue Time - {{workflow_step}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "expr": "sum(rate(github_actions_traces_span_metrics_duration_seconds_sum{workflow_step!~\".*queue.*|.*waiting.*\",service_name=~\"$service\",workflow_step=~\"$workflow_step\"}[5m])) by (workflow_step) / sum(rate(github_actions_traces_span_metrics_duration_seconds_count{workflow_step!~\".*queue.*|.*waiting.*\",service_name=~\"$service\",workflow_step=~\"$workflow_step\"}[5m])) by (workflow_step)",
          "refId": "B",
          "legendFormat": "Execution Time - {{workflow_step}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20,
            "stacking": {
              "mode": "normal"
            }
          }
        }
      },
      "gridPos": {
        "h": 9,
        "w": 24,
        "x": 0,
        "y": 18
      }
    },
    {
      "id": 5,
      "title": "Repository Health Score Comparison",
      "type": "barchart",
      "targets": [
        {
          "expr": "100 * (sum by (service_name) (github_actions_traces_span_metrics_calls_total{step_status=\"success\",service_name=~\"$service\"}) / sum by (service_name) (github_actions_traces_span_metrics_calls_total{service_name=~\"$service\"}))",
          "refId": "A",
          "legendFormat": "{{service_name}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 80
              },
              {
                "color": "green",
                "value": 95
              }
            ]
          },
          "custom": {
            "axisPlacement": "left",
            "barAlignment": 0,
            "drawStyle": "bars",
            "fillOpacity": 80,
            "gradientMode": "opacity"
          }
        }
      },
      "options": {
        "orientation": "horizontal",
        "legend": {
          "displayMode": "hidden"
        }
      },
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 27
      }
    },
    {
      "id": 6,
      "title": "Workflow Activity Heatmap",
      "type": "heatmap",
      "targets": [
        {
          "expr": "sum by (workflow_step) (increase(github_actions_traces_span_metrics_calls_total{service_name=~\"$service\",workflow_step=~\"$workflow_step\"}[1h]))",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            }
          }
        }
      },
      "options": {
        "calculate": false,
        "cellGap": 2,
        "cellValues": {},
        "color": {
          "exponent": 0.5,
          "fill": "dark-orange",
          "mode": "spectrum",
          "reverse": false,
          "scale": "exponential",
          "scheme": "Oranges",
          "steps": 64
        },
        "exemplars": {
          "color": "rgba(255,0,255,0.7)"
        },
        "filterValues": {
          "le": 1e-09
        },
        "legend": {
          "show": true
        },
        "rowsFrame": {
          "layout": "auto"
        },
        "tooltip": {
          "show": true,
          "yHistogram": false
        },
        "yAxis": {
          "axisPlacement": "left",
          "reverse": false,
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 27
      }
    }
  ],
  "links": [
    {
      "asDropdown": false,
      "icon": "external link",
      "includeVars": true,
      "keepTime": true,
      "tags": [
        "github",
        "exploration"
      ],
      "targetBlank": true,
      "title": "Explore Workflows",
      "tooltip": "Open exploration dashboard",
      "type": "dashboards",
      "url": "/d/gh-workflow-exploration"
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "version": 1
}