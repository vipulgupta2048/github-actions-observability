{
  "id": null,
  "title": "GitHub Actions Workflow Analysis",
  "tags": [
    "github",
    "actions",
    "workflows",
    "analysis",
    "patterns",
    "scalable"
  ],
  "timezone": "browser",
  "description": "Scalable workflow analysis dashboard optimized for enterprise environments tracking hundreds of repositories and workflows - focuses on patterns and trends",
  "panels": [
    {
      "id": 1,
      "title": "Top Failing Workflow Types",
      "type": "table",
      "targets": [
        {
          "expr": "topk(10, sum by (span_name) (github_actions_traces_span_metrics_calls_total{status_code!=\"STATUS_CODE_OK\"}))",
          "refId": "A",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "left"
          },
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 10},
              {"color": "red", "value": 50}
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 8,
      "title": "Most Active Repositories",
      "type": "table",
      "targets": [
        {
          "expr": "topk(10, sum by (service_name) (rate(github_actions_traces_span_metrics_calls_total[1h])) * 3600)",
          "refId": "A",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "displayName": "Executions/hour",
          "custom": {
            "align": "left"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Workflow Success Rate Trends",
      "type": "graph",
      "targets": [
        {
          "expr": "sum(rate(github_actions_traces_span_metrics_calls_total{status_code=\"STATUS_CODE_OK\"}[5m])) / sum(rate(github_actions_traces_span_metrics_calls_total[5m])) * 100",
          "refId": "A",
          "legendFormat": "Overall Success Rate"
        },
        {
          "expr": "avg by () (sum by (service_name) (rate(github_actions_traces_span_metrics_calls_total{status_code=\"STATUS_CODE_OK\"}[5m])) / sum by (service_name) (rate(github_actions_traces_span_metrics_calls_total[5m])) * 100)",
          "refId": "B", 
          "legendFormat": "Average Repository Success Rate"
        }
      ],
      "yAxes": [
        {
          "unit": "percent",
          "label": "Success Rate",
          "min": 0,
          "max": 100
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 3,
      "title": "Workflow Duration Percentiles",
      "type": "graph",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le) (rate(github_actions_traces_span_metrics_duration_seconds_bucket[5m])))",
          "refId": "A",
          "legendFormat": "50th percentile"
        },
        {
          "expr": "histogram_quantile(0.90, sum by (le) (rate(github_actions_traces_span_metrics_duration_seconds_bucket[5m])))",
          "refId": "B",
          "legendFormat": "90th percentile"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(github_actions_traces_span_metrics_duration_seconds_bucket[5m])))",
          "refId": "C",
          "legendFormat": "95th percentile"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(github_actions_traces_span_metrics_duration_seconds_bucket[5m])))",
          "refId": "D",
          "legendFormat": "99th percentile"
        }
      ],
      "yAxes": [
        {
          "unit": "s",
          "label": "Duration"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      }
    },
    {
      "id": 4,
      "title": "Repository Activity Heatmap",
      "type": "heatmap",
      "targets": [
        {
          "expr": "sum by (service_name) (increase(github_actions_traces_span_metrics_calls_total[1h]))",
          "refId": "A",
          "format": "heatmap"
        }
      ],
      "heatmap": {
        "xBucketSize": "1h",
        "yBucketSize": "1",
        "yAxis": {
          "unit": "short",
          "label": "Repository"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 16
      }
    },
    {
      "id": 5,
      "title": "Step Duration by Type (Heatmap)",
      "type": "graph",
      "targets": [
        {
          "expr": "sum by (span_name) (rate(github_actions_traces_span_metrics_duration_seconds_sum[5m])) / sum by (span_name) (rate(github_actions_traces_span_metrics_duration_seconds_count[5m]))",
          "refId": "A",
          "legendFormat": "{{span_name}}"
        }
      ],
      "yAxes": [
        {
          "unit": "s",
          "label": "Average Duration"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 26
      }
    },
    {
      "id": 6,
      "title": "Pipeline Run Status Distribution",
      "type": "piechart",
      "targets": [
        {
          "expr": "sum by (cicd_pipeline_run_status) (github_actions_traces_span_metrics_calls_total)",
          "refId": "A",
          "legendFormat": "{{cicd_pipeline_run_status}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 26
      }
    },
    {
      "id": 7,
      "title": "Repository Performance Summary",
      "type": "table",
      "targets": [
        {
          "expr": "sum by (service_name) (rate(github_actions_traces_span_metrics_calls_total[1h])) * 3600",
          "refId": "A",
          "format": "table"
        },
        {
          "expr": "sum by (service_name) (rate(github_actions_traces_span_metrics_calls_total{status_code=\"STATUS_CODE_OK\"}[1h])) / sum by (service_name) (rate(github_actions_traces_span_metrics_calls_total[1h])) * 100",
          "refId": "B",
          "format": "table"
        },
        {
          "expr": "sum by (service_name) (rate(github_actions_traces_span_metrics_duration_seconds_sum[1h])) / sum by (service_name) (rate(github_actions_traces_span_metrics_duration_seconds_count[1h]))",
          "refId": "C",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "left"
          }
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "Value #A"},
            "properties": [
              {"id": "displayName", "value": "Executions/hour"},
              {"id": "unit", "value": "short"}
            ]
          },
          {
            "matcher": {"id": "byName", "options": "Value #B"},
            "properties": [
              {"id": "displayName", "value": "Success Rate"},
              {"id": "unit", "value": "percent"}
            ]
          },
          {
            "matcher": {"id": "byName", "options": "Value #C"},
            "properties": [
              {"id": "displayName", "value": "Avg Duration"},
              {"id": "unit", "value": "s"}
            ]
          }
        ]
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 32
      }
    }
  ],
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "refresh": "30s"
}