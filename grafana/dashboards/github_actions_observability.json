{
  "id": null,
  "title": "GitHub Actions Overview & Observability",
  "tags": [
    "github",
    "actions",
    "overview",
    "ci/cd",
    "otel",
    "observability"
  ],
  "timezone": "browser",
  "description": "Comprehensive overview and observability dashboard for GitHub Actions workflows, jobs, and steps with OpenTelemetry - optimized for enterprise scale",
  "panels": [
    {
      "id": 1,
      "title": "Workflow Runs Overview",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(rate(github_actions_traces_span_metrics_calls_total[5m])) * 300",
          "refId": "A",
          "legendFormat": "Calls/5min"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "displayName": "Workflow Calls (5min)",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Success Rate",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(rate(github_actions_traces_span_metrics_calls_total{status_code=\"STATUS_CODE_OK\"}[5m])) / sum(rate(github_actions_traces_span_metrics_calls_total[5m])) * 100",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "displayName": "Success Rate",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "red", "value": 0},
              {"color": "yellow", "value": 80},
              {"color": "green", "value": 95}
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 6,
        "y": 0
      }
    },
    {
      "id": 3,
      "title": "Average Duration",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(rate(github_actions_traces_span_metrics_duration_seconds_sum[5m])) / sum(rate(github_actions_traces_span_metrics_duration_seconds_count[5m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "displayName": "Avg Duration",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 4,
      "title": "Active Workflows",
      "type": "stat",
      "targets": [
        {
          "expr": "count(count by (span_name) (github_actions_traces_span_metrics_calls_total))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "displayName": "Active Workflows",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 18,
        "y": 0
      }
    },
    {
      "id": 5,
      "title": "Workflow Execution Rate",
      "type": "graph",
      "targets": [
        {
          "expr": "sum(rate(github_actions_traces_span_metrics_calls_total[5m]))",
          "refId": "A",
          "legendFormat": "Total Rate"
        },
        {
          "expr": "sum(rate(github_actions_traces_span_metrics_calls_total{status_code=\"STATUS_CODE_OK\"}[5m]))",
          "refId": "B",
          "legendFormat": "Success Rate"
        },
        {
          "expr": "sum(rate(github_actions_traces_span_metrics_calls_total{status_code!=\"STATUS_CODE_OK\"}[5m]))",
          "refId": "C",
          "legendFormat": "Error Rate"
        }
      ],
      "yAxes": [
        {
          "unit": "short",
          "label": "Executions per second"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 6,
      "title": "Top Repositories by Activity",
      "type": "table",
      "targets": [
        {
          "expr": "topk(10, sum by (service_name) (github_actions_traces_span_metrics_calls_total))",
          "refId": "A",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "left"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      }
    },
    {
      "id": 7,
      "title": "Top Workflows by Activity",
      "type": "table",
      "targets": [
        {
          "expr": "topk(10, sum by (span_name) (github_actions_traces_span_metrics_calls_total))",
          "refId": "A",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "left"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      }
    },
    {
      "id": 8,
      "title": "Duration Percentiles",
      "type": "graph",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(github_actions_traces_span_metrics_duration_seconds_bucket[5m])) by (le))",
          "refId": "A",
          "legendFormat": "50th percentile"
        },
        {
          "expr": "histogram_quantile(0.90, sum(rate(github_actions_traces_span_metrics_duration_seconds_bucket[5m])) by (le))",
          "refId": "B",
          "legendFormat": "90th percentile"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(github_actions_traces_span_metrics_duration_seconds_bucket[5m])) by (le))",
          "refId": "C",
          "legendFormat": "95th percentile"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(github_actions_traces_span_metrics_duration_seconds_bucket[5m])) by (le))",
          "refId": "D",
          "legendFormat": "99th percentile"
        }
      ],
      "yAxes": [
        {
          "unit": "s",
          "label": "Duration"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 24
      }
    },
    {
      "id": 9,
      "title": "Workflow Steps Analysis",
      "type": "table",
      "targets": [
        {
          "expr": "sum by (span_name) (github_actions_traces_span_metrics_calls_total)",
          "refId": "A",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "left"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 32
      }
    },
    {
      "id": 10,
      "title": "Error Analysis",
      "type": "table",
      "targets": [
        {
          "expr": "sum by (status_code) (github_actions_traces_span_metrics_calls_total{status_code!=\"STATUS_CODE_OK\"})",
          "refId": "A",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "left"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 32
      }
    }
  ],
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "refresh": "10s"
}